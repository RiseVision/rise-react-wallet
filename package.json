{
  "name": "rise-wallet",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^v1.4.2",
    "@material-ui/icons": "^2.0.0",
    "@types/lodash": "^4.14.111",
    "@types/moment-timezone": "^0.5.6",
    "bip39": "^2.5.0",
    "classnames": "^2.2.5",
    "dpos-api-wrapper": "^1.3.2",
    "dpos-offline": "^1.6.0",
    "jdenticon": "^2.1.0",
    "lodash": "^4.17.10",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "mobx-router": "^0.0.13",
    "moment-timezone": "^0.5.21",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-intl": "^2.4.0",
    "react-scripts-ts": "2.16.0",
    "store": "^2.0.12",
    "svg-country-flags": "^1.2.1"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "riw:prepare": "rm -r tmp/transpiled/; tsc -p . --target ES6 --module esnext --jsx preserve --outDir tmp/transpiled",
    "riw:translate": "riw app translate",
    "riw:status": "riw app status",
    "tsc": "tsc --project tsconfig.prod.json --noEmit",
    "tslint": "npx tslint --fix --project tslint.prod.json 'src/**/*.{ts,tsx}'",
    "precommit": "npm run tsc && npm run tslint"
  },
  "pre-commit": [
  ],
  "riw": {
    "defaultLocale": "en",
    "targetLocales": [
      "de",
      "es",
      "et",
      "fr",
      "it",
      "hu",
      "nl",
      "pl",
      "ro",
      "ru",
      "uk",
      "zh",
      "zh"
    ],
    "translationsDatabaseFile": "riw-db.json",
    "sourceDirs": [
      "tmp/transpiled/**/*.js",
      "tmp/transpiled/**/*.jsx"
    ],
    "reactIntlAutoConfig": false,
    "outputMode": "file-per-locale",
    "translationsOutputFile": "src/translations/locales/[locale].json",
    "todoFile": "translations-todo.json"
  },
  "devDependencies": {
    "@types/bip39": "^2.4.0",
    "@types/bytebuffer": "^5.0.37",
    "@types/classnames": "^2.2.4",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.1",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.5",
    "@types/react-intl": "^2.3.8",
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "pre-commit": "^1.2.2",
    "riw": "^0.8.0",
    "typescript": "^2.9.1"
  },
  "prettier": {
    "singleQuote": true
  }
}
