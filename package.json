{
  "name": "rise-wallet",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^v1.2.0",
    "@material-ui/icons": "^1.1.0",
    "bip39": "^2.5.0",
    "classnames": "^2.2.5",
    "dpos-offline": "^1.5.0",
    "jdenticon": "^2.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-intl": "^2.4.0",
    "react-scripts-ts": "2.16.0",
    "svg-country-flags": "^1.2.1"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "riw:prepare": "rm -r tmp/transpiled/; tsc -p . --target ES6 --module esnext --jsx preserve --outDir tmp/transpiled",
    "riw:translate": "riw app translate",
    "riw:status": "riw app status"
  },
  "riw": {
    "defaultLocale": "en",
    "targetLocales": [
      "et",
      "ko"
    ],
    "translationsDatabaseFile": "riw-db.json",
    "sourceDirs": [
      "tmp/transpiled/**/*.js",
      "tmp/transpiled/**/*.jsx"
    ],
    "reactIntlAutoConfig": false,
    "outputMode": "file-per-locale",
    "translationsOutputFile": "src/translations/locales/[locale].json",
    "todoFile": "translations-todo.json"
  },
  "devDependencies": {
    "@types/bip39": "^2.4.0",
    "@types/bytebuffer": "^5.0.37",
    "@types/classnames": "^2.2.4",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.1",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.5",
    "@types/react-intl": "^2.3.8",
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "riw": "^0.8.0",
    "typescript": "^2.9.1"
  }
}
