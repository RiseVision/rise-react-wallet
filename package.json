{
  "name": "rise-wallet",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "yarn": "^1.10.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@ledgerhq/hw-transport-u2f": "^4.28.0",
    "@material-ui/core": "^v3.3.1",
    "@material-ui/icons": "^3.0.1",
    "@types/keyboardjs": "^2.4.0",
    "@types/socket.io-client": "^1.4.32",
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bignumber.js": "^7.2.1",
    "bip39": "^2.5.0",
    "classnames": "^2.2.6",
    "delay": "^4.1.0",
    "downshift": "^3.1.4",
    "dpos-api-wrapper": "^1.3.2",
    "dpos-ledger-api": "^1.1.0",
    "dpos-offline": "2.0.2",
    "is-electron": "^2.2.0",
    "jdenticon": "^2.1.0",
    "keyboardjs": "^2.5.1",
    "lodash": "^4.17.11",
    "mdi-material-ui": "^5.6.0",
    "mobx": "^5.5.2",
    "mobx-react": "^5.3.5",
    "mobx-router-rise": "^0.0.13",
    "moment": "^2.22.2",
    "query-string": "^6.2.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.7.2",
    "react-scripts-ts": "3.1.0",
    "risejs": "^1.4.1",
    "sinon": "^7.1.0",
    "socket.io-client": "^2.1.1",
    "store": "^2.0.12",
    "svg-country-flags": "^1.2.6",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3",
    "unique-random": "^2.0.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "deploy-nightly": "npm run build && gh-pages -d build -m \"Updated the nightly\"",
    "deploy-now": "yarn build; cd build && now && now alias rise-wallet",
    "test": "run-p --race serve test-cypress",
    "test-ci": "run-p --race serve test-cypress:record",
    "test-cypress": "cypress run",
    "test-cypress:record": "cypress run --record",
    "test-unit": "jest",
    "test-unit:prepare": "rm -rf tmp/transpiled/; tsc --module commonjs --target es2017",
    "test-unit:prepare-watch": "rm -rf tmp/transpiled/; tsc --watch --module commonjs --target es2017",
    "eject": "react-scripts-ts eject",
    "release": "yarn build && rm -f release.zip && cd build && zip -r ../release.zip *",
    "intl:prepare": "rm -rf tmp/transpiled/ tmp/messages/; tsc --jsx preserve && babel tmp/transpiled/ -o tmp/transpiled-bundle.js",
    "intl:export": "bash scripts/intl-export.sh",
    "intl:import": "bash scripts/intl-import.sh",
    "tsc": "tsc --project tsconfig.prod.json --noEmit",
    "tslint": "tslint --fix --project tslint.prod.json 'src/**/*.{ts,tsx}'",
    "precommit": "npm run tsc && npm run prettier && npm run tslint && npm run test-unit:prepare && npm run test-unit",
    "prettier": "prettier src/**.{ts,tsx} --write",
    "serve": "node src/serve.js & local-ssl-proxy --source 3000 --target 5000",
    "encrypt-fixtures": "travis encrypt-file cypress/fixtures/accounts.json cypress/fixtures/accounts.json.enc --add",
    "electron": "yarn build && rm -Rf desktop/app && cp -R build desktop/app && yarn electron-start",
    "electron-start": "cd desktop && yarn install && yarn start"
  },
  "pre-commit": [
    "precommit"
  ],
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@types/bip39": "^2.4.0",
    "@types/bytebuffer": "^5.0.37",
    "@types/classnames": "^2.2.6",
    "@types/jest": "^23.3.7",
    "@types/ledgerhq__hw-transport-u2f": "^4.21.0",
    "@types/lodash": "^4.14.117",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.12.0",
    "@types/query-string": "^6.1.1",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-helmet": "^5.0.7",
    "@types/react-intl": "^2.3.11",
    "babel-plugin-react-intl": "^3.0.1",
    "cypress": "^3.1.1",
    "express": "^4.16.4",
    "gh-pages": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "local-ssl-proxy": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3",
    "raf": "^3.4.0",
    "type-tagger": "^1.0.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "<rootDir>/tmp/transpiled/**/__tests__/**/*.js?(x)",
      "<rootDir>/tmp/transpiled/**/?(*.)+(spec|test).js?(x)"
    ],
    "testEnvironment": "jsdom",
    "setupFiles": [
      "raf/polyfill"
    ],
    "testURL": "http://localhost/"
  }
}
