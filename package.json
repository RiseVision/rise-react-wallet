    "react-helmet": "^5.2.0",
    "react-intl": "^2.7.2",
    "regenerator-runtime": "^0.13.2",
    "risejs": "^1.4.1",
    "sinon": "^7.2.2",
    "socket.io-client": "^2.2.0",
    "store": "^2.0.12",
    "svg-country-flags": "^1.2.6",
    "ts-jest": "^23.10.5",
    "typescript": "^3.5.2",
    "unique-random": "^2.0.0",
