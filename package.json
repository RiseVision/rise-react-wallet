    "bignumber.js": "^8.0.1",
    "bip39": "^2.5.0",
    "classnames": "^2.2.6",
    "delay": "^4.1.0",
    "downshift": "^3.1.7",
    "dpos-ledger-api": "^3.0.1",
    "dpos-offline": "^3.0.4",
    "file-saver": "^2.0.2",
    "inobounce": "^0.1.6",
    "is-mobile": "^2.0.0",
    "jdenticon": "^2.1.0",
    "keyboardjs": "^2.5.1",
    "lodash": "^4.17.11",
    "mdi-material-ui": "^5.7.0",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "mobx-router-rise": "^0.0.13",
